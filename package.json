{
  "name": "popcorn-time-desktop",
  "productName": "Popcorn Time",
  "version": "0.0.4",
  "description": "An experimental PopcornTime client",
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production FLAG_SEASON_COMPLETE=true FLAG_MANUAL_TORRENT_SELECTION=true parallel-webpack --config=webpack.config.build.js",
    "dev": "cross-env NODE_ENV=development FLAG_SEASON_COMPLETE=true concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && cd app && npm i",
    "hot-server": "node -r babel-register server.js",
    "lint": "eslint app test *.js",
    "bench-api": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --bail --slow 3000 --timeout 20000 --inline-diffs --async-only --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.benchmark.js",
    "test-api": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --bail --slow 3000 --timeout 10000 --inline-diffs --async-only --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --bail --slow 3000 --timeout 10000 --inline-diffs --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "test-screenshot": "cross-env API_USE_MOCK_DATA=true BABEL_DISABLE_CACHE=1 npm run build && mocha --bail --slow 3000 --timeout 10000 --inline-diffs --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "test": "npm run lint && npm run package && npm run test-api && npm run test-e2e",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.development",
    "package": "npm run build && build --publish never",
    "package-fast": "build --publish never",
    "precommit": "npm run test"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/popcorn-time-desktop.git"
  },
  "author": {
    "name": "Amila Welihinda",
    "email": "amilajack@gmail.com",
    "url": "https://github.com/amilajack"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/popcorn-time-desktop/issues"
  },
  "keywords": [
    "popcorn-time",
    "popcorn",
    "time",
    "client",
    "electron",
    "react",
    "redux",
    "es6"
  ],
  "build": {
    "appId": "popcorn-time-desktop",
    "app-category-type": "public.app-category.video",
    "asar": false,
    "win": {
      "iconUrl": "https://cdn.rawgit.com/amilajack/popcorn-time-desktop/master/app/icons/app.ico"
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "wcjs-prebuilt": {
    "runtime": "electron",
    "runtimeVersion": "v1.2.6",
    "version": "v0.2.6"
  },
  "homepage": "https://github.com/amilajack/popcorn-time-desktop#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
